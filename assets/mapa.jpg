<script type="text/javascript">
	var move_focus_obj;
	$(document).ready(function() {
	
		$('.area_aviso').bind('mouseover mousemove focus', function(e) {
	
			var area = $(this).attr("id");
			var popup= $('#popup_' + area);
			var tabla= $('#tabla_' + area);
			var indicador = $('#indicador_' + area);
			
			popup.css({display: 'inline'}).focus();
	
			var altura = (tabla.height() - 32) / 2;		
			indicador.css({top: altura});
	
			
			if(e.type != "focus")
				popup.css({left: e.pageX + 3, top: e.pageY - (altura + 15)});
			else
			{
				var offset = $("#contenedor_imagen_avisos").offset();
				var coords = $(this).attr("coords").split(",",2);
				var arriba = Number(offset.top) + (Number(coords[1]) - (altura + 15));
				var izquierda = Number(offset.left) + Number(coords[0]);
				popup.css({left: izquierda , top: arriba});				
			}
			
			$(this).attr("xlink:title", ""); 
			$(this).attr("title", ""); 
// 			if(e.type != "focus")
// 			{
// 				if(document.all) {tabla.focus();}
// 				move_focus_obj=tabla;
// 				setTimeout("move_focus_obj.focus()",0);	
// 			}
		});
	
		$('.area_aviso').bind('mouseout blur', function(e) {
			
			var area = $(this).attr("id");
			var popup= $('#popup_' + area);
					
			popup.css({display: 'none'});
			$(this).attr("title", $('#title_' + area).attr("title")); 
		});
	});

</script>

